{
  "data_sources": [
    {
      "name": "mootdx",
      "module": "simtradedata.data_sources.mootdx_provider.MootdxProvider",
      "priority": 1,
      "type": "local",
      "enabled": true,
      "options": {
        "base_path": "/data/mootdx",
        "cache_enabled": true,
        "timeout": 10,
        "retry_times": 3
      }
    },
    {
      "name": "qstock",
      "module": "simtradedata.data_sources.qstock_provider.QstockProvider",
      "priority": 2,
      "type": "online",
      "enabled": true,
      "options": {
        "api_key_secret": "secret_ref:qstock",
        "rate_limit": 200,
        "timeout": 15,
        "retry_times": 3
      }
    },
    {
      "name": "baostock",
      "module": "simtradedata.data_sources.baostock_provider.BaostockProvider",
      "priority": 3,
      "type": "online",
      "enabled": true,
      "options": {
        "rate_limit": 150,
        "timeout": 20,
        "retry_times": 3,
        "auto_login": true
      }
    }
  ]
}
